{
    "name": "@naverpay/hidash",
    "version": "0.0.3",
    "description": "improved lodash",
    "files": [
        "dist"
    ],
    "sideEffects": false,
    "exports": {
        "./isEmpty": {
            "import": {
                "types": "./dist/esm/isEmpty.d.mts",
                "default": "./dist/esm/isEmpty.mjs"
            },
            "require": {
                "types": "./dist/cjs/isEmpty.d.ts",
                "default": "./dist/cjs/isEmpty.js"
            }
        },
        "./size": {
            "import": {
                "types": "./dist/esm/size.d.mts",
                "default": "./dist/esm/size.mjs"
            },
            "require": {
                "types": "./dist/cjs/size.d.ts",
                "default": "./dist/cjs/size.js"
            }
        },
        "./keys": {
            "import": {
                "types": "./dist/esm/keys.d.mts",
                "default": "./dist/esm/keys.mjs"
            },
            "require": {
                "types": "./dist/cjs/keys.d.ts",
                "default": "./dist/cjs/keys.js"
            }
        },
        "./isNil": {
            "import": {
                "types": "./dist/esm/isNil.d.mts",
                "default": "./dist/esm/isNil.mjs"
            },
            "require": {
                "types": "./dist/cjs/isNil.d.ts",
                "default": "./dist/cjs/isNil.js"
            }
        },
        "./isArray": {
            "import": {
                "types": "./dist/esm/isArray.d.mts",
                "default": "./dist/esm/isArray.mjs"
            },
            "require": {
                "types": "./dist/cjs/isArray.d.ts",
                "default": "./dist/cjs/isArray.js"
            }
        },
        "./isObject": {
            "import": {
                "types": "./dist/esm/isObject.d.mts",
                "default": "./dist/esm/isObject.mjs"
            },
            "require": {
                "types": "./dist/cjs/isObject.d.ts",
                "default": "./dist/cjs/isObject.js"
            }
        },
        "./has": {
            "import": {
                "types": "./dist/esm/has.d.mts",
                "default": "./dist/esm/has.mjs"
            },
            "require": {
                "types": "./dist/cjs/has.d.ts",
                "default": "./dist/cjs/has.js"
            }
        }
    },
    "scripts": {
        "prepare": "lefthook install -f",
        "build": "pnpm run clean && vite build --config vite.config.mts && pnpm run build:dts",
        "release": "changeset publish",
        "build:dts": "tsup --config tsup.config.ts",
        "clean": "rm -rf dist",
        "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
        "lint:fix": "eslint '**/*.{js,jsx,ts,tsx}' --fix",
        "prettier": "prettier --check '**/*.{ts,tsx,js,mjs,cjs,jsx,json,yaml,yml}'",
        "prettier:fix": "prettier --write '**/*.{ts,tsx,js,mjs,cjs,jsx,json,yaml,yml}'",
        "md": "markdownlint '*.md'",
        "md:fix": "markdownlint --fix '*.md'",
        "test": "vitest run",
        "bench": "vitest bench --run",
        "coverage": "vitest run --coverage",
        "changeset-version": "changeset version && pnpm run md:fix"
    },
    "author": "yc.effort@navercorp.com",
    "license": "MIT",
    "packageManager": "pnpm@9.12.3",
    "devDependencies": {
        "@babel/plugin-transform-runtime": "^7.25.9",
        "@changesets/cli": "^2.27.9",
        "@naverpay/browserslist-config": "^1.6.1",
        "@naverpay/editorconfig": "^0.0.4",
        "@naverpay/eslint-config": "^1.0.7",
        "@naverpay/markdown-lint": "^0.0.3",
        "@naverpay/prettier-config": "^1.0.0",
        "@rollup/plugin-babel": "^6.0.4",
        "@types/lodash": "^4.17.13",
        "@types/node": "^20.17.6",
        "@vitest/coverage-istanbul": "2.1.4",
        "@vitest/ui": "^2.1.4",
        "browserslist-to-esbuild": "^2.1.1",
        "lefthook": "^1.8.2",
        "lodash": "^4.17.21",
        "rollup": "^4.24.3",
        "rollup-preserve-directives": "^1.1.2",
        "tsup": "^8.3.5",
        "vite": "^5.4.10",
        "vite-tsconfig-paths": "^5.0.1",
        "vitest": "^2.1.4"
    },
    "engines": {
        "node": ">=16.0.0"
    }
}
